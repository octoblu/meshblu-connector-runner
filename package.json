{
  "name": "meshblu-connector-runner",
  "version": "9.2.0",
  "description": "Meshblu connector runner",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "bin": {
    "meshblu-connector-runner": "./command.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/meshblu-connector-runner.git"
  },
  "keywords": [
    "Meshblu",
    "connector",
    "runner"
  ],
  "author": "Octoblu, Inc. <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/meshblu-connector-runner/issues"
  },
  "homepage": "https://github.com/octoblu/meshblu-connector-runner#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "portfinder": "^1.0.3",
    "server-destroy": "^1.0.1",
    "shmock": "^0.8.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "socket.io": "^1.4.6"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "bunyan": "^1.8.1",
    "coffee-script": "^1.10.0",
    "dashdash": "^1.13.1",
    "fs-cson": "^0.3.2",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "is-running": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.6.1",
    "meshblu": "^1.34.1",
    "meshblu-config": "^4.0.0",
    "octoblu-raven": "^3.1.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
